{
  "name": "ume-booking-service",
  "version": "1.1.17",
  "description": "",
  "type": "commonjs",
  "main": "build/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "jest --env=node --color",
    "build": "tsc --build",
    "lint": "eslint . --fix",
    "openapi": "npm run openapi:generator && npm run openapi:publish",
    "build:watch": "tsc --build --watch",
    "start:watch": "nodemon --exec npx ts-node build/index.js",
    "start": "cross-env NODE_ENV=production ts-node build/index.js",
    "start:test": "cross-env NODE_ENV=test concurrently  -p \"[{time}] [{name}]\" -n \"WATCH,SERVER,ORTHER\" -c \"blue.bold,green.bold,yellow.bold\" --kill-others \"npm run build:watch\" \"npm run start:watch\" -e NODE_ENV=test",
    "start:dev": "cross-env NODE_ENV=development concurrently  -p \"[{time}] [{name}]\" -n \"WATCH,SERVER,ORTHER\" -c \"blue.bold,green.bold,yellow.bold\" --kill-others \"npm run build:watch\" \"npm run start:watch\" -e NODE_ENV=development",
    "start:docker": "cross-env NODE_ENV=docker concurrently -p \"[{time}] [{name}]\" -n \"WATCH,SERVER,ORTHER\" -c \"blue.bold,green.bold,yellow.bold\" --kill-others \"npm run build:watch\" \"npm run start:watch\"",
    "openapi:generator": "openapi-generator-cli generate -g typescript-axios -i swagger.yml -o ./openapi --additional-properties npmVersion=%npm_package_version%,npmName=%npm_package_name%-openapi,supportsES6=true,withInterfaces=true --git-repo-id=%npm_package_name%-openapi --git-user-id=ume-software",
    "openapi:publish": "cd openapi && npm publish && cd ../",
    "dotenv:dev": "dotenv -e .env.development",
    "dotenv:prod": "dotenv -e .env.production",
    "dotenv:docker": "dotenv -e .env.docker",
    "prisma:dev:push": "npm run dotenv:dev prisma db push",
    "prisma:dev:migrate": "npm run dotenv:dev prisma migrate dev",
    "prisma:dev:studio": "npm run dotenv:dev prisma studio",
    "prisma:dev:pull": "npm run dotenv:dev prisma db pull",
    "prisma:prod:push": "npm run dotenv:prod prisma db push",
    "prisma:prod:migrate": "npm run dotenv:prod prisma migrate dev",
    "prisma:prod:studio": "npm run dotenv:prod prisma studio",
    "prisma:prod:pull": "npm run dotenv:prod prisma db pull",
    "prisma:docker:push": "npm run dotenv:docker prisma db push",
    "prisma:docker:migrate": "npm run dotenv:docker prisma migrate dev",
    "prisma:docker:studio": "npm run dotenv:docker prisma studio",
    "prisma:docker:pull": "npm run dotenv:docker prisma db pull",
    "prisma:generate": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/common": "^9.4.3",
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/cron": "^2.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.195",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "@types/sharp": "^0.31.1",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^36.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.1",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "chalk": "^5.3.0",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "cron": "^2.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-swagger-typescript": "^1.0.38",
    "firebase-admin": "^11.10.1",
    "install": "^0.13.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "npm": "^9.8.1",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.32.4",
    "socket.io": "^4.7.1",
    "supertest": "^6.3.3",
    "swagger-ui-dist": "^5.1.3",
    "uuid": "^9.0.0"
  }
}
