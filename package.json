{
  "name": "ume-identity-service",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "build/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "jest --env=node --color",
    "build": "npm run prisma:generate && tsc --build",
    "start": "ts-node build/index.js",
    "lint": "eslint . --fix",
    "build:watch": "tsc --build --watch",
    "start:watch": "nodemon --exec npx ts-node build/index.js",
    "dev": "concurrently  -p \"[{time}] [{name}]\" -n \"WATCH,SERVER,ORTHER\" -c \"blue.bold,green.bold,yellow.bold\" --kill-others \"npm run build:watch\" \"npm run start:watch\"",
    "openapi:generator": "openapi-generator-cli generate -g typescript-axios -i swagger.yml -o ./openapi --additional-properties npmVersion=%npm_package_version%,npmName=%npm_package_name%-openapi,supportsES6=true,withInterfaces=true --git-repo-id=%npm_package_name%-openapi --git-user-id=ume-software",
    "openapi:publish": "cd openapi && npm publish && cd ../",
    "openapi": "npm run openapi:generator && npm run openapi:publish",
    "prisma:push": "prisma db push",
    "prisma:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
